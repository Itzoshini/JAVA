import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner s = new Scanner(System.in);
	    System.out.println("Enter the number 1:");
	    int a = s.nextInt();
	   System.out.println("Enter the number 2:");
	    int b = s.nextInt();
	    
	   
	    int big = a>b?a:b ;
	    int lcm = big;
	    
	    while(true){
	        if(lcm%a==0 && lcm%b==0)
	        break;
	        else
	        lcm = lcm+big ;
	       
	        
	    }
	    System.out.println(lcm);
	}
}
